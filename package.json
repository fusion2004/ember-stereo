{
  "name": "ember-hifi",
  "version": "1.18.0",
  "description": "The easy way to play audio in your ember app.",
  "keywords": [
    "ember-addon",
    "ember-hifi",
    "audio",
    "hls",
    "html5",
    "howler",
    "sound",
    "mp3",
    "api",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nypublicradio/ember-hifi/"
  },
  "license": "MIT",
  "author": "Jeff Keen, Brian Whitton",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
