<DocsDemo as |demo|>
  <demo.example @name="stereo-volume.hbs">
    <div class="stereo-player-volume">
      <span class="range-container">
        <Input {{controls-stereo-volume}} type="range" />
        <button type="button" {{on 'click' (toggle-mute)}}>
          {{#if (gt (stereo-volume) 0)}}
          {{svg-jar "volume-low-outline" height=20 width=30}}
          <span>{{stereo-volume}}</span>
          {{else}}
          {{svg-jar "volume-mute-outline" height=20 width=30}}
          <span>{{stereo-volume}}</span>
          {{/if}}
        </button>
      </span>
    </div>
  </demo.example>
  {{demo.snippet "stereo-volume-control.hbs"}}
</DocsDemo>